<html>
<?php
if ($_SERVER['REQUEST_METHOD'] === 'POST' && isset($_POST['id'])) {
	$id=$_POST['id'];
} else {
	die('Invalid request');
}
?>
<head>
<meta http-equiv="refresh" content="0; url=https://downloads.sourceforge.net/project/resurrectionremix-pie/<?php base64_decode($id);?>" >    
<meta name="referrer" content="no-referrer">
</head>
</html>
